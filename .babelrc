{
  "plugins": [
      [
        "module-resolver", {
          "root": ["."],
          "alias": {
            "styles": "./styles"
          },
          "cwd": "babelrc"
      }],
      [
        "wrap-in-js",
        {
          "extensions": ["css$"]
        }
      ],
      ["inline-react-svg"],
      ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
    ],
    "presets": [
      "next/babel"
    ],
    "ignore": []
  }
